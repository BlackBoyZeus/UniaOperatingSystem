  ],
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "definition": "label_values(lidar_processing_latency_ms, instance)",
        "hide": 0,
        "includeAll": true,
        "label": "Device",
        "multi": false,
        "name": "device",
        "options": [],
        "query": {
          "query": "label_values(lidar_processing_latency_ms, instance)",
