# Node Environment Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_SESSION_TOKEN=your_session_token
AWS_PRIMARY_REGION=us-east-1
AWS_SECONDARY_REGIONS=us-west-2,eu-west-1
AWS_REPLICATION_ROLE_ARN=your_replication_role_arn
AWS_REPLICATION_BUCKET_ARN=your_replication_bucket_arn
AWS_ACCOUNT_ID=your_account_id
AWS_KMS_KEY_ID=your_kms_key_id

# DynamoDB Configuration
DYNAMODB_ENDPOINTS=http://localhost:8000
DB_REGION=us-east-1
DB_AUTO_SCALE=true
DB_MIN_CAPACITY=5
DB_MAX_CAPACITY=100
DB_BACKUP_ENABLED=true
DB_ENCRYPTION_KEY=your_encryption_key
DB_COMPLIANCE_MODE=standard
DB_PERFORMANCE_THRESHOLD=50

# S3 Configuration
S3_BUCKET_REGIONS=us-east-1,us-west-2
S3_BUCKET_NAME=tald-unia-assets

# CloudWatch Configuration
CLOUDWATCH_REGIONS=us-east-1
CLOUDWATCH_RETENTION_DAYS=30
CLOUDWATCH_METRIC_RESOLUTION=60

# Redis Configuration
REDIS_NODES=localhost:6379
REDIS_PASSWORD=your_redis_password
REDIS_TLS=true
REDIS_CLUSTER_ENABLED=true
REDIS_MAX_RETRIES=3
REDIS_TTL=900
REDIS_CLUSTER_NODES=redis-node1:6379,redis-node2:6379,redis-node3:6379

# WebRTC Configuration
STUN_SERVERS=stun:stun1.l.google.com:19302,stun:stun2.l.google.com:19302
TURN_SERVER_URL=turn:turn.tald-unia.com:3478
TURN_SERVER_USERNAME=your_turn_username
TURN_SERVER_CREDENTIAL=your_turn_credential
TURN_USERNAME=your_turn_username
TURN_CREDENTIAL=your_turn_credential
WEBRTC_MAX_PEERS=32
WEBRTC_TIMEOUT=30000
WEBRTC_ICE_GATHERING_TIMEOUT=5000
WEBRTC_MAX_RECONNECT_ATTEMPTS=3

# Fleet Configuration
FLEET_MAX_SIZE=32
FLEET_SYNC_INTERVAL=50
FLEET_MAX_RETRIES=3
FLEET_SYNC_TIMEOUT=1000
FLEET_PERFORMANCE_THRESHOLD=0.8
FLEET_NETWORK_QUALITY_THRESHOLD=0.7

# LiDAR Configuration
LIDAR_MAX_SCAN_RATE=30
LIDAR_MIN_RESOLUTION=0.01
LIDAR_MAX_RANGE=5.0
LIDAR_MAX_PROCESSING_TIME=50
LIDAR_MIN_POINT_DENSITY=1000
LIDAR_MAX_ERROR_RATE=0.001
LIDAR_MIN_CONFIDENCE_THRESHOLD=0.95

# Security Configuration
SECURITY_ENCRYPTION_ENABLED=true
SECURITY_AUDIT_LOGGING=true
SECURITY_COMPLIANCE_MODE=standard
SECURITY_FLEET_TRUST_THRESHOLD=80

# Monitoring Configuration
MONITORING_ENABLED=true
MONITORING_INTERVAL=60000
MONITORING_RETENTION_DAYS=30
MONITORING_ALERT_THRESHOLD=0.8