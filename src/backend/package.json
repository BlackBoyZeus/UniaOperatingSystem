{
  "name": "tald-unia-backend",
  "version": "1.0.0",
  "description": "Backend server for TALD UNIA gaming platform with real-time fleet management and LiDAR integration",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "security:audit": "npm audit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "3.0.0",
    "@elastic/elasticsearch": "8.9.0",
    "@nvidia/cuda": "12.0.0",
    "@opentelemetry/api": "1.4.0",
    "@types/device-capabilities": "^1.0.0",
    "@vulkan/vulkan-sdk": "1.3",
    "automerge": "2.0",
    "aws-kinesis-client": "2.5.0",
    "aws-sdk": "2.1450.0",
    "bcrypt": "^5.1.0",
    "compression": "1.7.4",
    "datadog-metrics": "1.2.0",
    "dotenv": "16.3.1",
    "events": "3.3.0",
    "express": "4.18.2",
    "express-correlation-id": "2.0.0",
    "express-rate-limit": "6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "2.0.0",
    "http-status": "^1.6.2",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "ioredis": "5.3.2",
    "jsonwebtoken": "^9.0.0",
    "node-tpm2": "^2.0.0",
    "opossum": "7.1.0",
    "performance-now": "^2.1.0",
    "prom-client": "14.2.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "7.2-alpine",
    "retry": "0.13.0",
    "uuid": "^9.0.0",
    "validator": "13.11.0",
    "ws": "^8.13.0",
    "zlib": "^1.0.5",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "29.0.0",
    "@testing-library/webrtc-mock": "^1.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "jest": "^29.0.0",
    "jest-retry": "^2.0.0",
    "mock-webrtc": "1.0.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "winston": "3.10.0",
    "winston-cloudwatch": "3.1.0"
  },
  "author": "TALD UNIA Team",
  "license": "MIT"
}