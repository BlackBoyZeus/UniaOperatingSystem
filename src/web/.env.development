VITE_API_BASE_URL=http://localhost:3000
VITE_API_VERSION=v1

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:3001

# WebRTC Configuration
VITE_WEBRTC_SIGNALING_URL=ws://localhost:3002
VITE_STUN_SERVER=stun:stun.l.google.com:19302
VITE_TURN_SERVER=turn:localhost:3478
VITE_TURN_USERNAME=dev_user
VITE_TURN_CREDENTIAL=dev_password

# Fleet Management
VITE_MAX_FLEET_SIZE=32

# LiDAR Configuration
VITE_LIDAR_SCAN_RATE=30
VITE_MOCK_LIDAR=true

# Performance Settings
VITE_NETWORK_LATENCY_TARGET=50
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Display Configuration
VITE_ENABLE_HDR=true
VITE_ENABLE_DEBUG_OVERLAY=true

# Development Mode Settings
VITE_MODE=development
VITE_DEBUG=true

# Feature Flags
VITE_ANALYTICS_ENABLED=false