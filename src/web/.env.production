# API Configuration
VITE_API_BASE_URL=https://api.tald-unia.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=5000
VITE_UPLOAD_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://ws.tald-unia.com
VITE_WEBSOCKET_RECONNECT_INTERVAL=1000
VITE_MAX_RECONNECT_ATTEMPTS=5
VITE_HEARTBEAT_INTERVAL=30000
VITE_SYNC_TIMEOUT=1000

# WebRTC Configuration
VITE_WEBRTC_SIGNALING_URL=wss://signaling.tald-unia.com
VITE_WEBRTC_ICE_SERVERS=[{"urls":["stun:stun.l.google.com:19302"]},{"urls":["turn:turn.tald-unia.com:3478"],"username":"${TURN_USERNAME}","credential":"${TURN_CREDENTIAL}"}]

# Fleet Management
VITE_MAX_FLEET_SIZE=32

# Display Configuration
VITE_ENABLE_HDR=true
VITE_COLOR_GAMUT=p3
VITE_COLOR_DEPTH=10

# Monitoring & Analytics
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true